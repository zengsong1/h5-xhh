{
  "name": "phoneproject",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "cookie": "^0.3.1",
    "core-js": "^2.6.5",
    "jsencrypt": "^3.0.0-rc.1",
    "swiper": "^4.5.0",
    "vant": "^2.1.3",
    "vue": "^2.6.10",
    "vue-awesome-swiper": "^3.1.3",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-lazyload": "^1.3.3",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "document": true,
      "localStorage": true,
      "window": true,
      "RSAUtils": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "arrow-parens": 0,
      "generator-star-spacing": 0,
      "no-constant-condition": 0,
      "no-redeclare": 0,
      "no-undef": 0,
      "no-unused-vars": [
        0,
        {
          "vars": "local",
          "args": "none"
        }
      ],
      "semi": [
        0
      ],
      "no-multiple-empty-lines": [
        0,
        {
          "max": 100
        }
      ],
      "no-mixed-spaces-and-tabs": [
        0
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
